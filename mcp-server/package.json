{
  "name": "@robinmordasiewicz/f5xc-terraform-mcp",
  "version": "3.8.22",
  "description": "MCP server for F5 Distributed Cloud Terraform provider - documentation, 270+ OpenAPI specs, subscription info, and addon activation workflows for AI assistants",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "f5xc-terraform-mcp": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:ts && npm run build:docs",
    "build:ts": "tsc",
    "build:docs": "node scripts/copy-docs.js",
    "sync-version": "node scripts/sync-version.js",
    "prepublish": "npm run sync-version && npm run build",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "f5",
    "f5xc",
    "f5-distributed-cloud",
    "terraform",
    "terraform-provider",
    "ai",
    "claude",
    "openapi",
    "infrastructure-as-code",
    "iac",
    "api-documentation"
  ],
  "author": "Robin Mordasiewicz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/robinmordasiewicz/terraform-provider-f5xc.git",
    "directory": "mcp-server"
  },
  "bugs": {
    "url": "https://github.com/robinmordasiewicz/terraform-provider-f5xc/issues"
  },
  "homepage": "https://github.com/robinmordasiewicz/terraform-provider-f5xc/tree/main/mcp-server#readme",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.1",
    "glob": "^11.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "mcpName": "io.github.robinmordasiewicz/f5xc-terraform-mcp"
}
