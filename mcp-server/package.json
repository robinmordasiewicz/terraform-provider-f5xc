{
  "name": "@robinmordasiewicz/f5xc-terraform-mcp",
  "version": "3.21.0",
  "description": "MCP server for F5 Distributed Cloud Terraform provider - documentation, 270+ OpenAPI specs, subscription info, and addon activation workflows for AI assistants",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "f5xc-terraform-mcp": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "manifest.json",
    "icon.png",
    "f5.svg"
  ],
  "scripts": {
    "build": "npm run build:ts && npm run build:docs",
    "build:ts": "tsc",
    "build:docs": "node scripts/copy-docs.js",
    "sync-version": "node scripts/sync-version.js",
    "prepublish": "npm run sync-version && npm run build",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:acceptance": "vitest run tests/acceptance/",
    "test:e2e": "vitest run tests/acceptance/resource-creation.test.ts tests/acceptance/determinism.test.ts --reporter=verbose",
    "test:e2e:resource-creation": "vitest run tests/acceptance/resource-creation.test.ts --reporter=verbose",
    "test:e2e:determinism": "vitest run tests/acceptance/determinism.test.ts --reporter=verbose",
    "validate:auth": "tsx scripts/validate-auth-integration.ts"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "f5",
    "f5xc",
    "f5-distributed-cloud",
    "terraform",
    "terraform-provider",
    "ai",
    "claude",
    "openapi",
    "infrastructure-as-code",
    "iac",
    "api-documentation"
  ],
  "author": "Robin Mordasiewicz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/robinmordasiewicz/terraform-provider-f5xc.git",
    "directory": "mcp-server"
  },
  "bugs": {
    "url": "https://github.com/robinmordasiewicz/terraform-provider-f5xc/issues"
  },
  "homepage": "https://github.com/robinmordasiewicz/terraform-provider-f5xc/tree/main/mcp-server#readme",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@robinmordasiewicz/f5xc-auth": "latest",
    "axios": "^1.7.0",
    "glob": "^11.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.10.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.52.0",
    "vitest": "^4.0.16"
  },
  "mcpName": "io.github.robinmordasiewicz/f5xc-terraform-mcp"
}
